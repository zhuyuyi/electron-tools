{
    "printWidth": 100,
    "tabWidth": 4, 
    "useTabs": false, 
    "semi": true, 
    "singleQuote": true,
    "trailingComma": "es5", 
    "bracketSpacing": false,
    "jsxBracketSameLine": false, 
    "arrowParens": "avoid", 
    "proseWrap": "always",
    "htmlWhitespaceSensitivity": "css",
    "overrides": [
        {
          "files": ".prettierrc",
          "options": { "parser": "json" }
        }
    ]
}
